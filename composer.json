{
    "name": "asm/cloudinary-extension",
    "type": "symfony-bundle",
    "description": "Cloudinary conector to Akeneo",
    "license": "OSL-3.0",
    "authors": [
        {"name": "ASM Web Services", "homepage": "http://www.asmws.com"}
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/jjdiaz/asm-cloudinary_connector.git"
        }
    ],
    "require": {
        "php": ">=7.1",
        "akeneo/pim-community-dev": "2.*",
        "worldia/textmaster-api": "0.7.*"
    },
    "require-dev": {
        "behat/behat": "^3.1.0",
        "friendsofphp/php-cs-fixer": "^2.8.2",
        "phpspec/phpspec": "^2.5.8"
    },
    "autoload": {
        "psr-4": {
            "Asm\\Bundle\\CloudinaryConnectorBundle\\": "src/"
        }
    },
    "branch-alias": {
        "dev-master": "1.0.x-dev",
        "dev-1.0": "1.0.x-dev"
    }
}
